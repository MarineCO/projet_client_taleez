{
	"manifest_version": 2,
	"name": "taleez",
	"version": "0.9",

	"description": "Une extension taleez",

	"browser_action": {
		"default_title": "Ataleez",
		"default_popup": "index.html"
	},

	"content_scripts": [
		{
			"matches": ["https://www.linkedin.com/*"],
			"js": ["app.js"]
		}
	],

	"author": "Rachdi Bassem , Ru tan , Marine Colonge",


	"permissions": [
		"tabs",
		"clipboardRead",
		"clipboardWrite",
		"storage"
	]
}